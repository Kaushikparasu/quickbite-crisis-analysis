        "tab = fact_ord.merge(dim_res,how = 'inner',on = 'restaurant_id')[['order_id','restaurant_id','period','city']]\n",
        "tab = tab.groupby(['period','city'])['order_id'].count().reset_index()\n",
        "\n",
        "# tab = tab.pivot(index = 'city',columns='period',values = 'order_id')\n",
        "tab\n",
        "\n",
        "\n",
        "fig = px.bar(\n",
        "    tab,\n",
        "    x='city',\n",
        "    y='order_id',\n",
        "    color='period',                  # 🔹 distinguishes each bar group\n",
        "    barmode='group',                 # 🔹 side-by-side bars\n",
        "    text_auto=True,\n",
        "    color_discrete_map={\n",
        "        'Pre-Crisis': 'seagreen',\n",
        "        'Crisis': 'indianred'\n",
        "    },\n",
        "    title='City-wise Orders (Pre-Crisis vs Crisis)'\n",
        ")\n",
        "\n",
        "\n",
        "fig.update_layout(\n",
        "    xaxis_title='City',\n",
        "    yaxis_title='Total Orders',\n",
        "    title_x=0.5,\n",
        "    plot_bgcolor='white',\n",
        "    legend=dict(title='', orientation='h', y=-0.2)\n",
        ")\n",
        "\n",
        "fig.update_traces(textposition='outside')\n",
        "fig.show()\n",
        "\n",
        "tab = tab.pivot(index = 'city',columns='period',values = 'order_id')\n",
        "tab\n",
        "\n",
        "\n",
        "tab['percent_decline'] = ((tab['Pre-Crisis']-tab['Crisis'])/tab['Pre-Crisis'])*100\n",
        "tab = tab.sort_values('percent_decline',ascending = False)\n",
        "tab['percent_decline'].head(5)\n",
        "\n"
      ]
    },
