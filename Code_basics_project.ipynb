        "ord_del = fact_ord.merge(fact_del,how = 'inner',on ='order_id')[['order_id','period','actual_delivery_time_mins','expected_delivery_time_mins']]\n",
        "\n",
        "ord_del['SLA_compliance'] = 0\n",
        "ord_del.loc[ord_del['actual_delivery_time_mins'] <= ord_del['expected_delivery_time_mins'],'SLA_compliance'] = 1\n",
        "\n",
        "ord_del=ord_del.groupby('period').agg(\n",
        "    {\n",
        "        'actual_delivery_time_mins':'mean',\n",
        "        'SLA_compliance':'sum',\n",
        "        'order_id':'count'\n",
        "    }\n",
        ")\n",
        "\n",
        "ord_del.rename(columns={'actual_delivery_time_mins': 'Avg Delivery Time (mins)',\n",
        "                        'SLA_compliance_percentage': 'SLA Compliance (%)'}, inplace=True)\n",
        "\n",
        "\n",
        "ord_del['SLA Compliance (%)'] = round((ord_del['SLA_compliance']/ord_del['order_id'])*100,2)\n",
        "ord_del = ord_del[['Avg Delivery Time (mins)','SLA Compliance (%)']].T\n",
        "ord_del.index.name = 'metric'\n",
        "ord_del['Crisis'] = round(ord_del['Crisis'],1)\n",
        "ord_del['Pre-Crisis'] = round(ord_del['Pre-Crisis'],1)\n",
        "\n",
        "ord_del['change'] = abs(ord_del['Crisis'] - ord_del['Pre-Crisis'])\n",
        "ord_del\n",
        "\n",
        "\n",
        "metrics = ['Avg Delivery Time (mins)', 'SLA Compliance (%)']\n",
        "pre_crisis = ord_del.loc[metrics, 'Pre-Crisis'].values\n",
        "crisis = ord_del.loc[metrics, 'Crisis'].values\n",
        "\n",
        "# Create figure\n",
        "fig = go.Figure()\n",
        "fig.add_trace(go.Bar(\n",
        "    y=metrics,\n",
        "    x=-pre_crisis,  # negative to push to left\n",
        "    orientation='h',\n",
        "    name='Pre-Crisis',\n",
        "    marker_color='seagreen',\n",
        "    text=pre_crisis,\n",
        "    texttemplate='%{text:.1f}',\n",
        "    textposition='outside'\n",
        "))\n",
