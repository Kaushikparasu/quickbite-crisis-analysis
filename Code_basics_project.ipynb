        "\n",
        "cust_with_more_ord = a[a>=5].reset_index()\n",
        "cust_ord_in_Crisis = fact_ord[fact_ord['period'] == 'Crisis'].groupby('customer_id')['order_id'].count().reset_index()\n",
        "a=cust_with_more_ord.merge(cust_ord_in_Crisis,how = 'left',on = 'customer_id').fillna(0)\n",
        "cust_above_5_ord_stopped_ord = a[a['order_id_y'] == 0]['customer_id'].nunique()\n",
