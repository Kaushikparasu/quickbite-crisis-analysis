        "ord_rat = fact_ord.merge(fact_rat,how = 'inner',on ='order_id')\n",
        "crisis_reviews = ord_rat[ord_rat['period'] == 'Crisis'][['review_text']]\n",
        "top_reviews=crisis_reviews.value_counts().reset_index()\n",
        "top_reviews\n",
        "\n",
        "\n",
        "\n",
        "fig = px.bar(\n",
        "    top_reviews.head(10),\n",
        "    x='count',\n",
        "    y='review_text',\n",
        "    text = 'count',\n",
        "    orientation='h',\n",
        "    color='count',\n",
        "    color_continuous_scale='Reds',\n",
        "    title='Top 10 Negative Review Themes During Crisis'\n",
        ")\n",
        "fig.update_layout(\n",
        "    plot_bgcolor='white',\n",
        "    title_x=0.5,\n",
        "    yaxis_title='Review Category',\n",
        "    xaxis_title='Count of Mentions'\n",
        ")\n",
        "fig.show()\n",
        "\n",
        "\n",
        "# as the reviews are somewhat simlar group them and chnage text."
      ]
