      "source": [
        "fact_ord['month_name'] = fact_ord['order_timestamp'].dt.month_name()\n",
        "fact_ord['month_number'] = fact_ord['order_timestamp'].dt.month\n",
        "fact_ord.groupby(['month_name','month_number'])['order_id'].count().reset_index().sort_values('month_number').set_index('month_name')['order_id']\n",
        "\n",
        "fact_ord['period'] = 'Crisis'\n",
        "fact_ord.loc[fact_ord['month_number'].isin([1,2,3,4,5]),'period'] = 'Pre-Crisis'\n",
        "\n",
        "monthly_orders = fact_ord.groupby('period').agg(\n",
        "    {\n",
        "        'order_id':'count',\n",
        "        'month_number':'nunique'\n",
        "    }\n",
        ")\n",
        "monthly_orders['average_orders'] = monthly_orders['order_id']/monthly_orders['month_number']\n",
        "\n",
        "monthly_orders\n",
        "\n",
        "# this is average orders in each period\n",
        "fig = px.pie(\n",
        "    monthly_orders,\n",
        "    values='average_orders',\n",
        "    names=monthly_orders.index,\n",
        "    color=monthly_orders.index,\n",
        "    hole=0.6,\n",
        "    title=\"Order Share by Phase\",\n",
        "    color_discrete_map={'Pre-Crisis':'seagreen', 'Crisis':'indianred'}\n",
        ")\n",
        "fig.update_traces(textinfo='percent+label',\n",
        "                  textposition='inside',\n",
        "                  textfont_size=14,\n",
        "                  marker_line_color='white',\n",
        "                  marker_line_width=1.5\n",
        ")\n",
        "fig.update_layout(title_x=0.5, showlegend=False)\n",
        "fig.show()\n",
        "\n",
        "pre_avg_ord = monthly_orders.loc['Pre-Crisis']['average_orders']\n",
        "cri_avg_ord = monthly_orders.loc['Crisis']['average_orders']\n",
        "\n",
        "decline_rate = ((pre_avg_ord-cri_avg_ord)/pre_avg_ord)*100\n",
        "decline_rate"
      ]
    },
