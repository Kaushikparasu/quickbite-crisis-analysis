      "source": [
        "# lets find the restaurants with 50+ orders pre crisis.\n",
        "ord_res = fact_ord.merge(dim_res,how = 'inner',on = 'restaurant_id')[['order_id','restaurant_id','restaurant_name','period']]\n",
        "ord_res=ord_res.groupby(['restaurant_name','period'])['order_id'].count().reset_index()\n",
        "\n",
        "\n",
        "ord_res=ord_res.pivot(index = 'restaurant_name',columns='period',values = 'order_id').sort_values('Pre-Crisis', ascending = False)\n",
        "res_above_50 = ord_res[ord_res['Pre-Crisis'] >= 50]\n",
        "\n",
        "\n",
        "\n",
        "res_above_50['percent_decline'] = ((res_above_50['Pre-Crisis'] - res_above_50['Crisis'])/res_above_50['Pre-Crisis'])*100\n",
        "top10_decline_res = res_above_50.sort_values('percent_decline', ascending = False).head(10)\n",
        "\n",
        "\n",
        "\n",
        "top10_decline = (\n",
        "    res_above_50\n",
        "    .sort_values('percent_decline', ascending=False)\n",
        "    .head(10)\n",
        "    .reset_index()\n",
        ")\n",
        "\n",
        "fig = px.bar(\n",
        "    top10_decline,\n",
        "    x='percent_decline',\n",
        "    y='restaurant_name',\n",
        "    orientation='h',\n",
        "    text='percent_decline',\n",
        "    color='percent_decline',\n",
        "    color_continuous_scale='Reds',\n",
        "    title='Top 10 Restaurants with Highest % Decline in Orders (Pre-Crisis → Crisis)',\n",
        "    labels={'percent_decline': 'Decline (%)', 'restaurant_name': 'Restaurant'}\n",
        ")\n",
        "\n",
        "fig.update_traces(\n",
        "    texttemplate='↓ %{text:.1f}%',\n",
        "    textposition='outside'\n",
        ")\n",
        "\n",
        "fig.update_layout(\n",
        "    xaxis_title='Percentage Decline in Orders',\n",
        "    yaxis_title='Restaurant Name',\n",
        "    coloraxis_showscale=False,\n",
        "    plot_bgcolor='white',\n",
        "    title_x=0.5\n",
        ")\n",
        "\n",
        "fig.show()\n",
        "\n",
        "top10_decline_res\n",
        "\n",
        "\n",
        "# Among restaurants with at least 50 pre-crisis orders, the top 10 suffered declines ranging from ~82% to 94% in total order volume during the crisis.\n",
        "# These high-volume, high-visibility restaurants likely contributed significantly to the overall decline in orders."
      ]
    },
