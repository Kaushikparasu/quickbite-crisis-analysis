        "monthly_rev = fact_ord[['order_id','total_amount','month_number','period']]\n",
        "monthly_rev=monthly_rev.groupby('period').agg(\n",
        "    {\n",
        "        'total_amount':'sum',\n",
        "        'month_number':'nunique'\n",
        "    }\n",
        ")\n",
        "\n",
        "\n",
        "fig = px.pie(monthly_rev,\n",
        "             values = 'total_amount',\n",
        "             names = monthly_rev.index,\n",
        "             hole = 0.7,\n",
        "             title = 'Total Revenue Across Pre-Crisis vs Crisis')\n",
