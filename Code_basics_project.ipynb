      "source": [
        "fact_ord['month_name'] = fact_ord['order_timestamp'].dt.month_name()\n",
        "fact_ord['month_number'] = fact_ord['order_timestamp'].dt.month\n",
        "fact_ord.groupby(['month_name','month_number'])['order_id'].count().reset_index().sort_values('month_number').set_index('month_name')['order_id']\n",
        "\n",
        "# draw this graph too to show the decline.\n",
        "data = fact_ord.groupby(['month_name','month_number'])['order_id'].count().reset_index().sort_values('month_number').set_index('month_name')['order_id']\n",
        "\n",
        "colors = ['Pre_crisis' if val > 10000 else 'Crisis' for val in data.values]\n",
        "\n",
        "fig = px.bar(\n",
        "    data,\n",
        "    x=data.index,\n",
        "    y=data.values,\n",
        "    text_auto=True,\n",
        "    color=colors,\n",
        "    color_discrete_map={'Pre_crisis':'seagreen', 'Crisis':'indianred'},\n",
        "    title=\"Number of orders per month\",\n",
        ")\n",
        "\n",
        "fig.update_layout(\n",
        "    xaxis_title=\"Month\",\n",
        "    yaxis_title=\"Total Orders\",\n",
        "    title_font=dict(size=20),\n",
        "    xaxis_tickangle=-45\n",
        ")\n",
        "\n",
        "fig.show()"
      ]
    },
