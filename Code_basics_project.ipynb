      "source": [
        "ord_rat = fact_ord.merge(fact_rat,how = 'inner',on ='order_id')[['order_id','month_name','month_number','period','rating']]\n",
        "ord_rat.groupby(['month_name','month_number'])['rating'].mean().reset_index().sort_values('month_number').set_index('month_name')['rating']\n",
        "\n",
        "# Group and sort\n",
        "avg_monthly_rating = (\n",
        "    ord_rat.groupby(['month_name','month_number','period'])['rating']\n",
        "    .mean()\n",
        "    .reset_index()\n",
        "    .sort_values('month_number')\n",
        ")\n",
        "\n",
        "avg_monthly_rating['rating'] = avg_monthly_rating['rating'].round(2)\n",
        "\n",
        "\n",
        "\n",
        "fig = px.line(\n",
        "    avg_monthly_rating,\n",
        "    x='month_name',\n",
        "    y='rating',\n",
        "    color='period',\n",
        "    markers=True,\n",
        "    color_discrete_map={'Pre-Crisis':'seagreen', 'Crisis':'indianred'},\n",
        "    title='Monthly Average Rating Trend'\n",
        ")\n",
        "fig.update_traces(line=dict(width=3))\n",
        "fig.show()\n",
        "\n",
        "\n",
        "# plot the above one.\n",
        "\n",
        "data = ord_rat.groupby('period')['rating'].mean().reset_index()\n",
        "\n",
        "fig = px.bar(\n",
        "    data,\n",
        "    x='period',\n",
        "    y='rating',\n",
        "    color='period',\n",
        "    text=data['rating'].round(2),\n",
        "    color_discrete_map={'Pre-Crisis': 'seagreen', 'Crisis': 'indianred'},\n",
        "    title='⭐ Average Customer Rating: Pre-Crisis vs Crisis'\n",
        ")\n",
        "\n",
        "# ✨ Styling the chart\n",
        "fig.update_traces(\n",
        "    width=0.3,\n",
        "    texttemplate='%{text:.2f}',       # round & display ratings\n",
        "    textposition='outside',           # show above bars\n",
        "    marker=dict(line=dict(color='black', width=1.2))  # outline bars\n",
        ")\n",
        "\n",
        "fig.update_layout(\n",
        "    title_x=0.5,\n",
        "    title_font=dict(size=20, family='Arial Black'),\n",
        "    xaxis_title='Phase',\n",
        "    yaxis_title='Average Rating',\n",
        "    yaxis=dict(showgrid=True, gridcolor='lightgrey', range=[0,5]), # keep rating scale visible\n",
        "    plot_bgcolor='white',\n",
        "    paper_bgcolor='white',\n",
        "    font=dict(size=13),\n",
        "    legend=dict(title='Phase', orientation='h', y=-0.25, x=0.3),\n",
        ")\n",
        "\n",
        "# Add annotation for insight\n",
        "fig.add_annotation(\n",
        "    text=\"⬇ Ratings fell significantly during the crisis period\",\n",
        "    x='Crisis',\n",
        "    y=data.loc[data['period'] == 'Crisis', 'rating'].values[0] + 0.2,\n",
        "    showarrow=True,\n",
        "    arrowhead=2,\n",
        "    arrowcolor='gray',\n",
        "    font=dict(size=12, color='black')\n",
        ")\n",
        "\n",
        "fig.show()\n",
        "# this is with respect to phase."
      ]
    },
